{
    "name": "Facehack",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["background.js"]
        }
    ],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "*://*"
    ]
}
